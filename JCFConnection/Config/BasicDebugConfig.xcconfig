COPY_PHASE_STRIP = NO
STRIP_INSTALLED_PRODUCT = NO
GCC_INLINES_ARE_PRIVATE_EXTERN = NO
GCC_SYMBOLS_PRIVATE_EXTERN = NO
STRIP_STYLE = debugging

//CPU优化等级
GCC_OPTIMIZATION_LEVEL = 0


